    'qft.criteria.cellestis.nomito.r'
    'qft.criteria.cellestis.usa.r'
    'trim.output.R'
    'qft.cens.R'
    'equal.lengths.R'
    'qft.interp.r'
    'trim.output.r'
    'tspot.cens.r'
    'tspot.criteria.10spot.r'
    'tspot.criteria.oxford.global.r'
    'tspot.criteria.oxford.usa.r'
    'tspot.interp.r'
